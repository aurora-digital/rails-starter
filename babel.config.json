{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": ["react-imported-component/babel"],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    }
  }
}
